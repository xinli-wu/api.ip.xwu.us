{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.mjs"
    }
  ],
  "builds": [
    {
      "src": "/src/index.mjs",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "NODE_ENV": "@node_env",
    "API_PORT": "@api_port"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ]
    }
  ]
}